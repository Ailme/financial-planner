{
  "name": "ailme/financial-planner",
  "description": "Yii2 App. Financial planner",
  "type": "project",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "~2.0.5",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "bower-asset/html5shiv": "*",
    "bower-asset/respond": "*",
    "kartik-v/yii2-widget-datepicker": "*",
    "kartik-v/yii2-field-range": "*",
    "marc-mabe/php-enum": "^2.3",
    "beberlei/assert": "^2.7"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3",
    "codeception/codeception": "^2.3"
  },
  "autoload": {
    "files": [
      "components/lib.php"
    ]
  },
  "config": {
    "process-timeout": 1800
  },
  "scripts": {
    "messages": "php yii message @app/config/message.php; php yii message @app/modules/admin/config/message.php; php yii message @app/modules/main/config/message.php; php yii message @app/modules/user/config/message.php"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
